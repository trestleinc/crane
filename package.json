{
	"name": "@trestleinc/crane",
	"version": "0.0.2-preview.0",
	"description": "Browser automation engine with integrated credential vault",
	"repository": "https://github.com/trestleinc/crane",
	"homepage": "https://github.com/trestleinc/crane#readme",
	"bugs": {
		"url": "https://github.com/trestleinc/crane/issues"
	},
	"license": "Apache-2.0",
	"keywords": [
		"convex",
		"crane",
		"browser",
		"automation",
		"blueprints",
		"vault",
		"credentials",
		"stagehand"
	],
	"type": "module",
	"private": false,
	"resolutions": {
		"convex": "^1.31.0"
	},
	"overrides": {
		"convex": "^1.31.0"
	},
	"scripts": {
		"build": "oxlint --fix && tsdown",
		"dev": "tsdown --watch",
		"clean": "rm -rf dist"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/shared/index.d.ts",
				"default": "./dist/shared/index.js"
			}
		},
		"./package.json": "./package.json",
		"./client": {
			"import": {
				"@convex-dev/component-source": "./src/client/index.ts",
				"types": "./dist/client/index.d.ts",
				"default": "./dist/client/index.js"
			}
		},
		"./server": {
			"import": {
				"types": "./dist/server/index.d.ts",
				"default": "./dist/server/index.js"
			}
		},
		"./handler": {
			"import": {
				"types": "./dist/handler/index.d.ts",
				"default": "./dist/handler/index.js"
			}
		},
		"./convex.config": {
			"import": {
				"@convex-dev/component-source": "./src/component/convex.config.ts",
				"types": "./dist/component/convex.config.d.ts",
				"default": "./dist/component/convex.config.js"
			}
		},
		"./executor": {
			"import": {
				"types": "./dist/executor/index.d.ts",
				"default": "./dist/executor/index.js"
			}
		},
		"./shared": {
			"import": {
				"types": "./dist/shared/index.d.ts",
				"default": "./dist/shared/index.js"
			}
		}
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE"
	],
	"dependencies": {
		"@convex-dev/workflow": "^0.3.3",
		"@effect/platform": "0.94.1",
		"@logtape/logtape": "1.3.5",
		"effect": "3.19.14",
		"jose": "6.1.3"
	},
	"peerDependencies": {
		"convex": "^1.32.2",
		"@anthropic-ai/sdk": "0.71.2",
		"@browserbasehq/stagehand": "3.0.7"
	},
	"devDependencies": {
		"@trestleinc/convex-oxlint": "0.1.0",
		"@edge-runtime/vm": "5.0.0",
		"@microsoft/api-extractor": "7.55.2",
		"@tanstack/db": "0.5.16",
		"@types/node": "25.0.3",
		"@types/sql.js": "1.4.9",
		"convex": "1.32.2",
		"dotenv": "17.2.3",
		"fake-indexeddb": "6.2.5",
		"lib0": "0.2.117",
		"oxlint": "1.36.0",
		"publint": "0.3.16",
		"tsdown": "0.19.0-beta.1",
		"typescript": "5.9.3",
		"yjs": "13.6.28",
		"zod": "4.3.4"
	},
	"main": "./dist/shared/index.js",
	"types": "./dist/shared/index.d.ts",
	"module": "./dist/shared/index.js",
	"publishConfig": {
		"access": "public"
	}
}
